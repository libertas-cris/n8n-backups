        "jsCode": "function calcularInvestimento(valor, regiao) {\n    // Mapeia as regiões para os multiplicadores correspondentes\n    var multiplicadoresAte3Mil = {\n        'Norte': 70,\n        'Nordeste': 63,\n        'Centro oeste': 48,\n        'Sudeste': 33,\n        'Sul': 29\n    };\n\n    var multiplicadores10Mil = {\n        'Norte': 49,\n        'Nordeste': 44.1,\n        'Centro oeste': 33.6,\n        'Sudeste': 33,\n        'Sul': 29\n    };\n\n    // Função auxiliar para extrair o número do valor\n    function extrairNumero(valorStr) {\n        var numero = valorStr.match(/\\d+/);\n        return numero ? parseInt(numero[0]) : 0;\n    }\n        var numero = extrairNumero(valor);\n  var multi3mil = multiplicadoresAte3Mil[regiao];\n  var multi10mil = multiplicadores10Mil[regiao];\n  \n        if (valor.includes(\"Sim, menos de 3 mil reais\")){\n            return [multi3mil * 1500, multi3mil * 3000];\n          \n        } else if (valor.includes(\"Sim, de 3 até 10 mil reais\")) {\n            return [multi3mil * 3000, multi10mil * 10000];\n          \n        } else if (valor.includes(\"Sim, mais de 10 mil reais\")) {\n            return multi10mil * 10000;\n          \n        } else if (valor.includes(\"Não, mas quero\")) {\n            return \"Planeje seu investimento com um valor específico.\";\n        } else if (valor.includes(\"Não e nem planejo\")) {\n            return \"Não há investimento a ser feito.\";\n        } else {\n            return \"Entrada inválida.\";\n        }\n    } \n\n\n// Exemplo de uso\nvar valor = $input.all()[0].json['Investe em anúncios'];\nvar regiao = $('Code2').all()[0].json['regiao'];\nvar investimento = calcularInvestimento(valor, regiao);\n\nreturn {valor,\n       regiao,\n       investimento};\n"
      },
